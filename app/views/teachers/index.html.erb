<% @card_title = "Relação de " + @teachers.model_name.human(count: 2) %>
<% @btn_new = btn_new(new_teacher_path) %>

<div class="table-responsive">
	<%= table_for(@teachers) do %>
		<% field_table(@teachers.model.human_attribute_name('cpf'))  { |f| f.cpf } %>
		<% field_table(@teachers.model.human_attribute_name('name'))  { |f| f.name } %>
		<% btn_form %>
	<% end %>
</div>
